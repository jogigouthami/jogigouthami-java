import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner s=new Scanner(System.in); 
        int n1=s.nextInt();
        int n2=s.nextInt();
        int p=n1*n2;
        if(p>1000)
        {
           System.out.printf("%d\n",n1+n2);
        }
        else
       {
           System.out.printf("%d\n",n1*n2);
       }  
    }
}
