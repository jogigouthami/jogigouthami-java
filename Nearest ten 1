import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int r=n%10;
        if(r>=5)
      {
          System.out.printf("%d\n",n+(10-r));
      }
        else
      {
          System.out.printf("%d\n",n-r);
      } 
  
    }
}
