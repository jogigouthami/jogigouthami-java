import java.util.*;
 class Test {
    
        void show(int ...a)
        {
            System.out.println("var-arg method "+Arrays.toString(a));
        }
        public static void main(String[] args)
        {
         Test t1=new Test();
         t1.show();
         t1.show(23,56);
         t1.show(10,3,66,88);
         }
}
