import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     Scanner s=new Scanner(System.in);
        int p=s.nextInt();
        int r=p/4;    
        if(p>=100 && p<=200)
      {
        System.out.printf("%d\n%d\n%d\n%d\n",r,r,r,r+p%4);
      }
        else
      {
       System.out.println("INVALID INPUT");
      }
    
    }
}
