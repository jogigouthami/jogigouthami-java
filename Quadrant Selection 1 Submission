import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
        int x=s.nextInt();
        int y=s.nextInt();
        if(x>0 && y>0)
            System.out.print(1);
        if(x<0 && y>0)
            System.out.print(2);
        if(x<0 && y<0)
            System.out.print(3);
        if(x>0 && y<0)
            System.out.print(4);
        
        
        
    }
}
